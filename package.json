{
	"name": "nexjs-ws.template.server.ts.node",
	"version": "0.0.1",
	"license": "MIT",
	"main": "dist/main.js",
	"scripts": {
		"prebuild": "rimraf dist",
		"build": "cross-env NODE_ENV=production nest build",
		"start": "cross-env NODE_ENV=development nest start",
		"start:dev": "cross-env NODE_ENV=development nest start --watch",
		"start:debug": "cross-env NODE_ENV=development nest start --debug --watch",
		"start:prod": "cross-env NODE_ENV=production node dist/main",
		"nex:ts-ws-gen-client": "  npx nex ts ws gen-client   -s ./ -o ../tradin.api.wsclient-ts     -n @tradin/api.wsclient            -i -b",
		"nex:ts-ws-up-client": "   npx nex ts ws up-client    -s ./ -o ../tradin.api.wsclient-ts/src                             -v 0.0.1    -b",
		"nex:cs-ws-gen-client": "  npx nex cs ws gen-client   -s ./ -o ../tradin.api.wsclient-cs     -n tradin.api.wsclient             -i -b",
		"nex:cs-ws-up-client": "   npx nex cs ws up-client    -s ./ -o ../tradin.api.wsclient-cs/src                             -v 0.0.1    -b",
		"httpclient:gen-ts": "npx nex ts http gen-client -s ./ -o ../tradin.api.httpclient-ts   -n @tradin/api.httpclient          -i -b",
		"httpclient:gen-cs": "npx nex cs http gen-client -s ./ -o ../tradin.api.httpclient-cs   -n tradin.api.httpclient           -i -b",
		"doc:angular": "npx compodoc -p tsconfig.json -s -d documentation/angular",
		"doc:typedoc": "npx typedoc --out documentation/typedoc src --mode modules",
		"doc:serve-angular": "serve documentation/angular",
		"doc:serve-typedoc": "serve documentation/typedoc"
	},
	"dependencies": {
		"@nestjs/common": "^7.3.2",
		"@nestjs/core": "^7.3.2",
		"@nestjs/platform-express": "^7.3.2",
		"@nestjs/platform-socket.io": "^7.3.2",
		"@nestjs/serve-static": "^2.1.3",
		"@nestjs/swagger": "^4.5.12",
		"@nestjs/websockets": "^7.3.2",
		"@nexjs/wsserver": "^1.0.1",
		"@types/mongodb": "^3.5.25",
		"agenda": "^3.1.0",
		"bcryptjs": "^2.4.3",
		"class-transformer": "^0.2.3",
		"class-validator": "^0.12.2",
		"dotenv-flow": "^3.2.0",
		"express-session": "^1.17.1",
		"helmet": "^3.23.3",
		"jsonwebtoken": "^8.5.1",
		"module-alias": "^2.2.2",
		"moment": "^2.29.2",
		"mongodb": "^3.5.9",
		"morgan": "^1.10.0",
		"reflect-metadata": "^0.1.13",
		"rxjs": "^6.6.0",
		"strongly-typed-events": "^1.6.13",
		"swagger-ui-dist": "^3.28.0",
		"swagger-ui-express": "^4.1.4",
		"typedoc": "^0.17.8"
	},
	"devDependencies": {
		"@nestjs/cli": "^7.4.1",
		"@compodoc/compodoc": "^1.1.11",
		"@nexjs/cli": "^1.1.0",
		"@types/agenda": "",
		"@types/bcryptjs": "^2.4.2",
		"@types/express": "^4.17.7",
		"@types/express-session": "^1.17.0",
		"@types/jsonwebtoken": "^8.5.0",
		"@types/lodash": "^4.14.157",
		"@types/module-alias": "^2.0.0",
		"@types/multer": "^1.4.3",
		"@types/node": "^14.0.20",
		"@types/socket.io": "^2.1.8",
		"@types/uuid": "^8.0.0",
		"cross-env": "^7.0.2",
		"rimraf": "^3.0.2",
		"serve": "^11.3.2",
		"ts-loader": "^8.0.0",
		"ts-node": "^8.10.2",
		"tsc-watch": "^4.2.9",
		"typescript": "^3.9.6"
	},
	"_moduleAliases": {
		"@": "./dist"
	}
}